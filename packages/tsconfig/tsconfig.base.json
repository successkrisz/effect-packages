{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": false,
		"esModuleInterop": false,
		"skipLibCheck": true
	},
	"include": ["src"]
}
